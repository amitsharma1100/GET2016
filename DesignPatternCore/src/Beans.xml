<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="carFacade" class="com.Facade.CarFacade">
		<property name="carDAO" ref="carDAO"></property>
	</bean>
	<bean id="createCarServlet" class="com.Controller.CreateCarServlet">
		<property name="carVO" ref="carVO"></property>
	</bean>
	<bean id="carDAO" class="com.DAO.CarDAO">
	</bean>
	<bean id="carVO" class="com.VO.CarVO">
	</bean>
</beans>